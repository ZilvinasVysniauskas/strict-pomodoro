{
    "compilerOptions": {
        "sourceMap": true,
        "jsx": "react",
        "module": "es6",
        "target": "es6",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictBindCallApply": true,
    },
    "include": ["src/**/*ts"],
    "exclude": ["node_modules"]
}